    "import apache_beam as beam\n",
    "\n",
    "p1 = beam.Pipeline()\n",
    "\n",
    "Collection = (\n",
    "    p1\n",
    "    |beam.io.ReadFromText('../datasets/poem.txt')\n",
    "    |beam.FlatMap(lambda record: record.split(' '))\n",
    "    |beam.io.WriteToText('../datasets/output/result.txt')\n",
    ")\n",
    "p1.run()"
